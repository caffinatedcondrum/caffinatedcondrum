#include <stdio.h>

#define _CRT_NO_WARNINGS_SECURE_
#define WEIGHT 200
#define MERCURY 37.8
#define VENUS 90.7
#define MARS 37.7
#define JUPITER 236.0
#define SATURN 91.6
#define URANUS 88.9
#define NEPTUNE 112.0

int main()
{
    printf("%10|6.1lf\n", "Planet", "Weight");
    printf("%10|6s\n", "---------", "-------");
    printf("%10|6.1lf\n", "Mercury", WEIGHT * MERCURY/100.0);
    
}
